<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.tt.qzy.view">

    <uses-permission android:name="android.permission.VIBRATE"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/CommonViewStyle">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity
            android:name=".activity.UserEditorsActivity"
            android:theme="@style/CommonViewStyle">
        </activity>
        <activity
            android:name=".activity.SettingsActivity"
            android:theme="@style/CommonViewStyle">
        </activity>
        <activity
            android:name=".activity.OffLineMapActivity"
            android:theme="@style/CommonViewStyle">
        </activity>
        <activity android:name=".activity.MainAboutActivity"
                  android:theme="@style/CommonViewStyle">
        </activity>
        <activity android:name=".activity.TellPhoneActivity"
                  android:theme="@style/CommonViewStyle">
        </activity>
        <activity android:name=".activity.ContactsActivity"
                  android:theme="@style/CommonViewStyle">
        </activity>
    </application>

</manifest>