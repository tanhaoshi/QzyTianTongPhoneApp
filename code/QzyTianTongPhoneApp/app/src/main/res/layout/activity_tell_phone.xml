<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:background="@drawable/bg_tonghua"
    tools:context="com.tt.qzy.view.activity.TellPhoneActivity">

    <LinearLayout
        android:orientation="vertical"
        android:layout_width="match_parent"
        android:layout_height="248dp">

        <ImageView
            android:layout_marginTop="50dp"
            android:src="@drawable/mine_default_avatar"
            android:layout_gravity="center_horizontal"
            android:layout_width="80dp"
            android:layout_height="80dp"/>

        <TextView
            android:text="陌生联系人"
            android:textSize="18sp"
            android:layout_marginTop="16dp"
            android:layout_gravity="center_horizontal"
            android:textColor="@color/white"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"/>

        <LinearLayout
            android:layout_marginTop="7dp"
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="19dp">

            <TextView
                android:layout_weight="1"
                android:text="广东深圳"
                android:textColor="@color/white"
                android:gravity="end"
                android:layout_centerHorizontal="true"
                android:layout_marginRight="12dp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"/>

            <TextView
                android:id="@+id/textView"
                android:textColor="@color/white"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerHorizontal="true"
                android:layout_weight="1"
                android:text="手机"/>

        </LinearLayout>

        <TextView
            android:layout_marginTop="7dp"
            android:text="正在拨号"
            android:textColor="@color/white"
            android:gravity="center"
            android:layout_width="match_parent"
            android:layout_height="19dp"/>

    </LinearLayout>

    <LinearLayout
        android:layout_weight="1"
        android:orientation="vertical"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">

            <LinearLayout
                android:layout_marginLeft="7dp"
                android:layout_weight="1"
                android:gravity="center"
                android:orientation="vertical"
                android:layout_width="wrap_content"
                android:layout_height="57dp">

                <TextView
                    android:text="1"
                    android:textColor="@color/white"
                    android:textSize="27sp"
                    android:layout_width="match_parent"
                    android:gravity="center"
                    android:layout_height="wrap_content"/>

                <ImageView
                    android:src="@drawable/ic_dial_action_vm"
                    android:layout_width="24dp"
                    android:gravity="center"
                    android:layout_height="18dp"/>

            </LinearLayout>

            <LinearLayout
                android:layout_marginLeft="7dp"
                android:layout_weight="1"
                android:gravity="center"
                android:orientation="vertical"
                android:layout_width="wrap_content"
                android:layout_height="57dp">

                <TextView
                    android:text="2"
                    android:textColor="@color/white"
                    android:textSize="27sp"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"/>

                <TextView
                    android:text="ABC"
                    android:textColor="@color/white"
                    android:textSize="12sp"
                    android:layout_width="24dp"
                    android:layout_height="18dp"/>

            </LinearLayout>

            <LinearLayout
                android:layout_marginLeft="7dp"
                android:layout_marginRight="7dp"
                android:layout_weight="1"
                android:gravity="center"
                android:orientation="vertical"
                android:layout_width="wrap_content"
                android:layout_height="57dp">

                <TextView
                    android:text="3"
                    android:textColor="@color/white"
                    android:textSize="27sp"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"/>

                <TextView
                    android:text="DEF"
                    android:textColor="@color/white"
                    android:textSize="12sp"
                    android:layout_width="24dp"
                    android:layout_height="18dp"/>

            </LinearLayout>

        </LinearLayout>

        <LinearLayout
            android:layout_marginTop="7dp"
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">

            <LinearLayout
                android:layout_marginLeft="7dp"
                android:layout_weight="1"
                android:gravity="center"
                android:orientation="vertical"
                android:layout_width="wrap_content"
                android:layout_height="57dp">

                <TextView
                    android:text="4"
                    android:textColor="@color/white"
                    android:textSize="27sp"
                    android:layout_width="match_parent"
                    android:gravity="center"
                    android:layout_height="wrap_content"/>

                <TextView
                    android:text="GHI"
                    android:textColor="@color/white"
                    android:layout_width="24dp"
                    android:gravity="center"
                    android:layout_height="18dp"/>

            </LinearLayout>

            <LinearLayout
                android:layout_marginLeft="7dp"
                android:layout_weight="1"
                android:gravity="center"
                android:orientation="vertical"
                android:layout_width="wrap_content"
                android:layout_height="57dp">

                <TextView
                    android:text="5"
                    android:textColor="@color/white"
                    android:textSize="27sp"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"/>

                <TextView
                    android:text="ABC"
                    android:textColor="@color/white"
                    android:textSize="12sp"
                    android:layout_width="24dp"
                    android:layout_height="18dp"/>

            </LinearLayout>

            <LinearLayout
                android:layout_marginLeft="7dp"
                android:layout_marginRight="7dp"
                android:layout_weight="1"
                android:gravity="center"
                android:orientation="vertical"
                android:layout_width="wrap_content"
                android:layout_height="57dp">

                <TextView
                    android:text="6"
                    android:textColor="@color/white"
                    android:textSize="27sp"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"/>

                <TextView
                    android:text="DEF"
                    android:textColor="@color/white"
                    android:textSize="12sp"
                    android:layout_width="24dp"
                    android:layout_height="18dp"/>

            </LinearLayout>

        </LinearLayout>

        <LinearLayout
            android:layout_marginTop="7dp"
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">

            <LinearLayout
                android:layout_marginLeft="7dp"
                android:layout_weight="1"
                android:gravity="center"
                android:orientation="vertical"
                android:layout_width="wrap_content"
                android:layout_height="57dp">

                <TextView
                    android:text="7"
                    android:textColor="@color/white"
                    android:textSize="27sp"
                    android:layout_width="match_parent"
                    android:gravity="center"
                    android:layout_height="wrap_content"/>

                <TextView
                    android:text="PQRS"
                    android:textColor="@color/white"
                    android:layout_width="100dp"
                    android:gravity="center"
                    android:layout_height="18dp"/>

            </LinearLayout>

            <LinearLayout
                android:layout_marginLeft="7dp"
                android:layout_weight="1"
                android:gravity="center"
                android:orientation="vertical"
                android:layout_width="wrap_content"
                android:layout_height="57dp">

                <TextView
                    android:text="8"
                    android:textColor="@color/white"
                    android:textSize="27sp"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"/>

                <TextView
                    android:text="TUV"
                    android:textColor="@color/white"
                    android:gravity="center"
                    android:textSize="12sp"
                    android:layout_width="100dp"
                    android:layout_height="18dp"/>

            </LinearLayout>

            <LinearLayout
                android:layout_marginLeft="7dp"
                android:layout_marginRight="7dp"
                android:layout_weight="1"
                android:gravity="center"
                android:orientation="vertical"
                android:layout_width="wrap_content"
                android:layout_height="57dp">

                <TextView
                    android:text="9"
                    android:textColor="@color/white"
                    android:textSize="27sp"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"/>

                <TextView
                    android:text="WXYZ"
                    android:textColor="@color/white"
                    android:gravity="center"
                    android:textSize="12sp"
                    android:layout_width="100dp"
                    android:layout_height="18dp"/>

            </LinearLayout>

        </LinearLayout>

        <LinearLayout
            android:layout_marginTop="7dp"
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">

            <TextView
                android:text="*"
                android:layout_marginLeft="7dp"
                android:textColor="@color/white"
                android:textSize="27sp"
                android:layout_weight="1"
                android:layout_width="30dp"
                android:gravity="center"
                android:layout_height="57dp"/>

            <TextView
                android:layout_marginLeft="7dp"
                android:text="0"
                android:gravity="center"
                android:layout_weight="1"
                android:textColor="@color/white"
                android:textSize="27sp"
                android:layout_width="30dp"
                android:layout_height="57dp"/>

            <TextView
                android:text="#"
                android:layout_marginLeft="7dp"
                android:layout_marginRight="7dp"
                android:gravity="center"
                android:layout_weight="1"
                android:textColor="@color/white"
                android:textSize="27sp"
                android:layout_width="30dp"
                android:layout_height="57dp"/>

        </LinearLayout>

        <ImageView
            android:layout_gravity="center_horizontal"
            android:src="@drawable/bohao_big_red"
            android:layout_marginTop="10dp"
            android:layout_width="53dp"
            android:layout_height="53dp"/>

    </LinearLayout>

</LinearLayout>
